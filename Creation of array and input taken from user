#include<stdio.h>
int main(){
	int a[100],i,n;
	printf("Enter the size of array");
	scanf("%d",&n);
	if (n<100){
	printf("Enter the elements in the array:%d-",(i+1));
	for(i=0;i<n;i++)
	{
		scanf("%d",&a[i]);
	}
	printf("Now the final array is:\n");
	for(i=0;i<n;i++)
	{
		printf("%d\n",a[i]);
	}
        }
        else
	{
	printf("Invalid size of array");

        }
	return 0;

}
